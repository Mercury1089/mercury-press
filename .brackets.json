{
    "sass.enabled": false,
    "sass.compiler": "sass",
    "path": {
        "sass/style.sass": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../",
                "includePaths": [
                    "../sass/_partials",
                    "../sass/bourbon",
                    "../sass/neat"
                ],
                "sourceComments": false,
                "outputStyle": "nested"
            }
        }
    }
}